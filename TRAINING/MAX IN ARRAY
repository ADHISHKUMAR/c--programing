#include<stdio.h>
int main(void)
{
	int a[5],n,i,j,temp;
	printf("ENTER THE SIZE=");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("value=");
		scanf("%d",&a[i]);
	}
		for(i=0;i<n;i++)
		{
				for(j=i+1;j<n;j++)
				{
					if(a[i]<a[j])
					{
						temp=a[j];
					}
					else
					{
						temp=a[i];
					}
				}
		}
	printf("max=%d",temp);
}
